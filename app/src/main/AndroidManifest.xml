<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.unistrong.mobileprojectmanagement">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_management"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_management"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.activity.A_Welcome"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.activity.A_Home"
            android:launchMode="singleTask">

        </activity>

        <service android:name="com.login.api.S_DownLoadApp"/>
        <!--com.unistrong.mobileprojectmanagement.ui.activity.A_Welcome-->
    </application>

</manifest>